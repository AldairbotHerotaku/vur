# description	: openjdk-bin version
# homepage	: https://download.java.net

name=openjdk8-bin
version=8
release=1
source="https://download.java.net/openjdk/jdk8u41/ri/openjdk-8u41-b04-linux-x64-14_jan_2020.tar.gz"

build() {
       ls
	cd java-se-8u41-ru
	mkdir -p $PKG/usr/lib
	mkdir -p $PKG/usr/bin
	mkdir -p $PKG/usr/conf
	mkdir -p $PKG/usr/include
	mkdir -p $PKG/usr/jmods	
	cp -rv lib $PKG/usr/
	cp -rv bin $PKG/usr/
	cp -rv include $PKG/usr/
}
